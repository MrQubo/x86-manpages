DEST[191:128] := SRC1[191:128] BITWISE AND SRC2[191:128]
DEST[255:192] := SRC1[255:192] BITWISE AND SRC2[255:192]
DEST[MAXVL-1:256] := 0
.EE

.SS VANDPD (VEX.128 ENCODED VERSION)  href="andpd.html#vandpd--vex-128-encoded-version-"
